#include <stdio.h>

   int main() 
   {
        int myArray[3][3] = {{1, 11, 111}, {2, 22, 222}, {3, 33, 333}};
        int i, j;
        
        for(i = 0; i < 3; i++){
           for(j = 0; j < 3; j++){
              printf("My arrat[%d][%d]: %d\n", i, j, myArray[i][j]);
           }
        }
        return 0;
}
